keywords = {3D Portable devices,estereoscopy,video artifacts},
title = {{Stereoscopy Artifacts on Portable Auto-estereoscopic Displays: What Matters?}},
url = {http://enpub.fulton.asu.edu/resp/vpqm/vpqm09/Proceedings\_VPQM09/Papers/Boev\_vpqm\_09.pdf}
}
